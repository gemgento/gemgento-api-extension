<?xml version="1.0" encoding="UTF-8" ?>
<config>
<modules>
    <Gemgento_Order>
        <version>0.0.1</version>
    </Gemgento_Order>
</modules>
<frontend>
    <routers>
        <order>
            <use>standard</use>
            <args>
                <module>Gemgento_Order</module>
                <frontName>order</frontName>
            </args>
        </order>
    </routers>
</frontend>
<global>
    <models>
        <order>
            <class>Gemgento_Order_Model</class>
            <resourceModel>order_resource</resourceModel>
        </order>
        <order_resource>
            <class>Gemgento_Order_Model_Resource</class>
            <deprecatedNote>order_mysql4</deprecatedNote>
        </order_resource>
    </models>
    <resources>
        <gemgento_order_setup>
            <setup>
                <module>Gemgento_Order</module>
                <class>Gemgento_Order_Model_Resource_Setup</class>
            </setup>
        </gemgento_order_setup>
    </resources>
    <fieldsets>
        <sales_convert_quote>
            <gemgento_id>
                <to_order>*</to_order>
            </gemgento_id>
        </sales_convert_quote>
        <sales_convert_order>
            <gemgento_id>
                <to_quote>*</to_quote>
            </gemgento_id>
        </sales_convert_order>
    </fieldsets>
</global>
</config> 