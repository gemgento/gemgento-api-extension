<config>
	<modules>
		<IG_FlatShipping5>
			<version>1.0.1</version>
		</IG_FlatShipping5>
	</modules>
	
	<global>
		<helpers>
			<ig_flatshipping5>
				<class>IG_FlatShipping5_Helper</class>
			</ig_flatshipping5>
        </helpers>
		<models>
			<ig_flatshipping5>
				<class>IG_FlatShipping5_Model</class>
			</ig_flatshipping5>
		</models>
		<blocks>
			<ig_flatshipping5>
				<class>IG_FlatShipping5_Block</class>
			</ig_flatshipping5>
			<checkout>
				<rewrite>
					<onepage_shipping_method_available>IG_FlatShipping5_Block_Available</onepage_shipping_method_available>
				</rewrite>
			</checkout>
		</blocks>
	</global>
	
	<frontend>
		<layout>
			<updates>
				<ig_flatshipping5>
					<file>ig_flatshipping5.xml</file>
				</ig_flatshipping5>
			</updates>
		</layout>
	</frontend>
	
	<default>
		<carriers>
			<flatrate>
				<infotext></infotext>
			</flatrate>
            <flatrate2>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <model>ig_flatshipping5/carrier_flatrate2</model>
                <name>Fixed 2</name>
                <price>5.00</price>
                <title>Flat Rate</title>
                <type>I</type>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
				<infotext></infotext>
                <handling_type>F</handling_type>
            </flatrate2>
			<flatrate3>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <model>ig_flatshipping5/carrier_flatrate3</model>
                <name>Fixed 3</name>
                <price>5.00</price>
                <title>Flat Rate</title>
				<infotext></infotext>
                <type>I</type>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <handling_type>F</handling_type>
            </flatrate3>
			<flatrate4>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <model>ig_flatshipping5/carrier_flatrate4</model>
                <name>Fixed 4</name>
                <price>5.00</price>
				<infotext></infotext>
                <title>Flat Rate</title>
                <type>I</type>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <handling_type>F</handling_type>
            </flatrate4>
			<flatrate5>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <model>ig_flatshipping5/carrier_flatrate5</model>
                <name>Fixed 5</name>
                <price>5.00</price>
				<infotext></infotext>
                <title>Flat Rate</title>
                <type>I</type>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <handling_type>F</handling_type>
            </flatrate5>
		</carriers>
	</default>
</config>
